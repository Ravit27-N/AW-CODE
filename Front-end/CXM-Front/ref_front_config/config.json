    {
      "name": "Setting",
      "remoteEntry": "${FRONT_REMOTE_ENTRY_URL}/cxm-setting/remoteEntry.js",

      "routePath": "cxm-setting",
      "ngModuleName": "CxmSettingModule",
      "remoteName": "cxmSetting"
    },
    {
      "name": "Analytics",
      "remoteEntry": "${FRONT_REMOTE_ENTRY_URL}/cxm-analytics/remoteEntry.js",

      "routePath": "cxm-analytics",
      "ngModuleName": "CxmAnalyticsModule",
      "remoteName": "cxmAnalytics"
    }
  ]
}
